{
  "name": "@jadetree/ui",
  "version": "0.1.16",
  "description": "Jade Tree UI Components",
  "module": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build-css-scss": "postcss src/scss/index.scss src/scss/*.scss --base src/scss --dir css --ext css",
    "build-css-min": "postcss src/scss/index.scss src/scss/*.scss --base src/scss --dir css --ext min.css --env production",
    "build-css": "npm-run-all -p build-css-scss build-css-min",
    "build-dist": "rollup -c",
    "build-js": "tsc",
    "build": "npm-run-all clean build-css build-js build-dist",
    "clean-css": "rm -rf css",
    "clean-dist": "rm -rf dist",
    "clean-js": "rm -rf lib types",
    "clean": "npm-run-all -p clean-css clean-dist clean-js",
    "lint": "eslint ./src --ext .ts",
    "lint-css": "stylelint \"./src/**/*.scss\"",
    "prepare": "npm run build",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asymworks/jadetree-ui.git"
  },
  "author": {
    "name": "Jonathan Krauss",
    "email": "jkrauss@asymworks.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/asymworks/jadetree-ui/issues"
  },
  "homepage": "https://github.com/asymworks/jadetree-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@csstools/postcss-sass": "^5.0.1",
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.15",
    "@storybook/html": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jsdom": "^20.0.1",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "css-loader": "^5.2.7",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-loader": "^7.0.2",
    "postcss-scss": "^4.0.6",
    "rollup": "^3.7.5",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^10.1.1",
    "storybook-addon-mock": "^3.2.0",
    "style-loader": "^2.0.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "qinu": "^2.0.0"
  },
  "files": [
    "/css",
    "/dist",
    "/lib",
    "/types"
  ]
}
